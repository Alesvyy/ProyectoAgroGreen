            <table>
                <tr>
                
                  
                </tr>
                <tr>
                    <th>Nombre</th> 
                    <th>Primer Apellido</th>
                    <th>Segundo Apellido</th>
                    <th>Tipo de identificación</th>
                    <th>Identificación</th>
                    <th>Teléfono</th>
                    <th>Correo Electrónico</th>
                    <th>Contraseña</th>
                    <th>Fotografía</th>
                    <th>Tipo de usuario</th>
                    <th button onclick="location.reload()" class="actualizardatos" ></button> </th>
                    <th button onclick="location.reload()" class="remover "></button></th>
                    
                   
                    
                </tr>
            
                    <% registrousuarios.forEach(registro =>{ %> 
                        <tr> 
                        <td> <%= registro.usuario %> </td> 
                        <td> <%= registro.primerapellido %> </td> 
                        <td> <%= registro.segundoapellido %> </td> 
                        <td> <%= registro.IDidentificacion %> </td> 
                        <td> <%= registro.identificacion %> </td> 
                        <td> <%= registro.telefono %> </td> 
                        <td> <%= registro.correoelectronico %> </td> 
                        <td> <%= registro.password %> </td> 
                        <td> <%= registro.fotografia %> </td> 
                        <td> <%= registro.IDusuario %> </td> 
                        </tr> 
